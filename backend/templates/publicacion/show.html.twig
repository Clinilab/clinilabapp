{% extends 'base.html.twig' %}

{% block title %}Publicacion{% endblock %}

{% block body %}
    <h1>Publicacion</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ publicacion.id }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ publicacion.tipo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ publicacion.descripcion }}</td>
            </tr>
            <tr>
                <th>FechaRegistro</th>
                <td>{{ publicacion.fechaRegistro ? publicacion.fechaRegistro|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ publicacion.activo ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('publicacion_index') }}">back to list</a>

    <a href="{{ path('publicacion_edit', {'id': publicacion.id}) }}">edit</a>

    {{ include('publicacion/_delete_form.html.twig') }}
{% endblock %}
