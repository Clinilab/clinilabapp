{% extends 'base.html.twig' %}

{% block title %}Servicio{% endblock %}

{% block body %}
    <h1>Servicio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ servicio.id }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ servicio.codigo }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ servicio.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ servicio.activo ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('servicio_index') }}">back to list</a>

    <a href="{{ path('servicio_edit', {'id': servicio.id}) }}">edit</a>

    {{ include('servicio/_delete_form.html.twig') }}
{% endblock %}
