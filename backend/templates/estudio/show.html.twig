{% extends 'base.html.twig' %}

{% block title %}Estudio{% endblock %}

{% block body %}
    <h1>Estudio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ estudio.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ estudio.nombre }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ estudio.codigo }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ estudio.activo ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('estudio_index') }}">back to list</a>

    <a href="{{ path('estudio_edit', {'id': estudio.id}) }}">edit</a>

    {{ include('estudio/_delete_form.html.twig') }}
{% endblock %}
